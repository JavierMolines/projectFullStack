{
	"name": "dockermultipleproject",
	"version": "2.0.0",
	"description": "## Development Require ``` - Docker - Terminal (kitty ğŸ˜‰) ```",
	"packageManager": "npm@11.6.2",
	"workspaces": [
		"apps/*"
	],
	"scripts": {
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"lint:prettier": "prettier --write ./apps/**/*.{tsx,js,jsx,json,md,html,css}",
