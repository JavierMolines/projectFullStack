		"dev": "turbo run dev",
		"build": "turbo run build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"husky": "9.1.7",
		"jav-blast-setup": "1.0.31",
		"prettier": "3.8.1",
		"turbo": "2.8.3"
	}
}
